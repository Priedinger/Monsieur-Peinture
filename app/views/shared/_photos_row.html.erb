<%= link_to "Ajouter des photos", new_photo_path %>

<div class="photos">
      <% @all_photos.each do |photo| %>
        <%= cl_image_tag photo.photos.first.key, width: 100, height: 100%>
        <%= link_to "Supprimer", photo_path(photo.id), method: :delete, :remote => true, data: {confirm: "Supprimer dÃ©finitivement ?", controller: 'destroy', action: 'destroy#delete' } %>
        <%= link_to "Associer la photo", edit_photo_path(photo.id) %>
        <% if photo.realisation_id? %>
          <%= photo.realisation_id %>
        <% end %>
      <br>
      <% end %>
</div>
